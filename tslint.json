{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        // "max-line-length": {
        //     "options": [
        //         120
        //     ]
        // },
        "semicolon": [
            false,
            "always",
            "ignore-interfaces"
        ],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false
        // "no-console": {
        //     "severity": "warning",
        //     "options": [
        //         "debug",
        //         "info",
        //         "log",
        //         "time",
        //         "timeEnd",
        //         "trace"
        //     ]
        // }
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}